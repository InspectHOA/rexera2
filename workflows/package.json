{
  "name": "@rexera/workflows",
  "version": "1.0.0",
  "description": "n8n workflow definitions for Rexera 2.0",
  "main": "src/index.ts",
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "clean": "rm -rf dist n8n-workflows/*.backup.json",
    "workflow": "npx tsx src/cli/workflow-cli.ts",
    "workflow:list": "npx tsx src/cli/workflow-cli.ts list",
    "workflow:test": "npx tsx src/cli/workflow-cli.ts test",
    "workflow:validate": "npx tsx src/cli/workflow-cli.ts validate",
    "workflow:backup": "npx tsx src/cli/workflow-cli.ts backup",
    "workflow:test-payoff": "npx tsx src/cli/workflow-cli.ts test-payoff",
    "workflow:import-payoff": "npx tsx src/cli/workflow-cli.ts import-payoff"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.2.0",
    "tsx": "^4.0.0"
  },
  "files": [
    "n8n-workflows",
    "src",
    "README.md"
  ],
  "keywords": [
    "n8n",
    "workflows",
    "automation",
    "real-estate",
    "rexera"
  ]
}