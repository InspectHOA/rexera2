# E2E Test Configuration
# Copy to .env and fill in your values

# Test User Credentials (Required)
TEST_EMAIL=test-user@yourdomain.com
TEST_PASSWORD=your-secure-password

# Environment URLs (Optional - defaults provided)
FRONTEND_URL=http://localhost:3000
API_URL=http://localhost:3001

# Browser Configuration (Optional)
HEADLESS=true          # Set to false to see browser during testing
TIMEOUT=30000          # Test timeout in milliseconds

# Supabase (for direct DB verification if needed)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key