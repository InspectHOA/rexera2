{
  "version": 2,
  "name": "rexera-api",
  "functions": {
    "src/server.ts": {
      "runtime": "nodejs18.x",
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/server.ts"
    }
  ],
  "env": {
    "SUPABASE_URL": "@supabase-url",
    "SUPABASE_SERVICE_ROLE_KEY": "@supabase-service-role-key"
  }
}
